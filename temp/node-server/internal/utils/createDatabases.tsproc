export default async (statement) => {
  const query = `
<#list root.listaModelos as model>
  CREATE CACHED TABLE IF NOT EXISTS ${model.nomeMin} (
    <#list model.listaTodosCampos as campo>
      ${root.getSnippet("createCampoSQL", campo)},
    </#list>
    PRIMARY KEY (<#list root.model.getListaCamposComAConfigIgualA("isId","true") as campo>${campo.nome}<#sep>, </#sep></#list>),
  );
</#list>
  `

  if(!query.trim()) return

  await statement.execute(query)
}